{
  "MediaSettings": {
    "AllowImageFileTypes": "jpg,jpeg,png,gif",
    "ImageFolder": "media",
    "NoImage": "/images/no-image.png"
  },
  "ApiConfigurations": {
    "ApiTitle": "Media API",
    "ApiName": "coding_hub_microservices_api",
    "ApiVersion": "v1",
    "ApiBaseUrl": "http://media.api",
    "IdentityBaseUrl": "http://localhost:6002",
    "IssuerUri": "http://identity.api"
  },
  "ElasticConfigurations": {
    "Uri": "http://elasticsearch:9200",
    "Username": "elastic",
    "Password": "admin"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Authentication": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  }
}